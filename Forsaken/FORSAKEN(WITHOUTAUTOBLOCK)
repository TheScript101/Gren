bg.BackgroundTransparency = 0.6
bg.Parent = loadingGui

-- Big centered text label
local label = Instance.new("TextLabel")
label.Parent = bg
label.Size = UDim2.new(1, 0, 0, 100)
label.Position = UDim2.new(0, 0, 0.5, -50) -- center vertically
